{
  "name": "with-prisma-nest",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "start nestjs api": "nx serve api --watch",
    "prisma:generate": "npx prisma generate --schema libs/prisma-schema-one/prisma && npx prisma generate --schema libs/prisma-schema-two/prisma",
    "prisma-migrate": "dotenv -e ../.env -- npx prisma migrate dev --schema libs/prisma-schema-one/prisma && npx prisma migrate dev --schema libs/prisma-schema-two/prisma",
    "pnpm-instal": "pnpm install --no-frozen-lockfile",
    "pnpm-auto-update": "pnpm update",
    "nxupdate": "nx migrate latest",
    "nxmigrate": "nx migrate --run-migrations"
  },
  "private": true,
  "dependencies": {
    "@nestjs/common": "^11.1.6",
    "@nestjs/core": "^11.1.6",
    "@nestjs/platform-express": "^11.1.6",
    "@prisma/client": "^6.13.0",
    "axios": "^1.11.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "dotenv": "^17.2.1",
    "dotenv-cli": "10.0.0",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@nestjs/schematics": "^11.0.7",
    "@nestjs/testing": "^11.1.6",
    "@nx/eslint": "22.0.2",
    "@nx/eslint-plugin": "22.0.2",
    "@nx/jest": "22.0.2",
    "@nx/js": "22.0.2",
    "@nx/nest": "22.0.2",
    "@nx/node": "22.0.2",
    "@nx/webpack": "22.0.2",
    "@nx/workspace": "22.0.2",
    "@types/jest": "30.0.0",
    "@types/node": "~24.2.0",
    "@typescript-eslint/eslint-plugin": "8.43.0",
    "@typescript-eslint/parser": "8.43.0",
    "eslint": "9.33.0",
    "eslint-config-prettier": "10.1.8",
    "jest": "30.0.5",
    "jest-environment-node": "^30.0.5",
    "jest-util": "30.0.5",
    "nx": "22.0.2",
    "prettier": "^3.6.2",
    "prisma": "^6.13.0",
    "ts-jest": "29.4.1",
    "ts-node": "10.9.2",
    "typescript": "5.9.2"
  }
}
